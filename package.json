{
  "name": "@lwtlab/detect-gpu",
  "version": "0.1.0",
  "description": "detect GPU on Windows/macOS",
  "main": "dist/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "build": "run-script-os",
    "build:default": "rm -rf dist && tsc && cp -r src/bin dist",
    "build:win32": "cmd /c \"IF EXIST dist (RMDIR /S /Q dist)\" && tsc && XCOPY /S /I src\\bin dist\\bin",
    "test": "jest",
    "package": "npm pack",
    "publish": "npm publish"
  },
  "keywords": [],
  "author": "lwtlab",
  "license": "MIT",
  "jest": {
    "preset": "ts-jest",
    "testEnvironment": "node"
  },
  "devDependencies": {
    "@types/jest": "^29.5.12",
    "@types/node": "^22.1.0",
    "jest": "^29.7.0",
    "run-script-os": "^1.1.6",
    "ts-jest": "^29.2.4",
    "ts-node": "^10.9.2",
    "typescript": "^5.5.4"
  },
  "dependencies": {
    "electron-util": "0.17.2"
  }
}
